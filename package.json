{
  "name": "lets-search",
  "displayName": "lets-search",
  "description": "Quick search on browser",
  "version": "0.0.1",
  "publisher": "guilhermeg2k",
  "repository": {
    "type": "git",
    "url": "https://github.com/guilhermeg2k/lets-search"
  },
  "bugs": {
    "url": "https://github.com/guilhermeg2k/lets-search/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lets-search.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lets-search.search",
        "title": "Lets Search: Search on the browser"
      }
    ],
    "configuration": {
      "title": "lets-search",
      "properties": {
        "lets-search.engines": {
          "type": "array",
          "default": [
            {
              "key": "g",
              "url": "https://www.google.com/search?q={%query%}"
            }
          ]
        }
      }
    },
    "keybindings": [
      {
        "command": "lets-search.search",
        "key": "ctrl+shift+q"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}